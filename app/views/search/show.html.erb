<%= turbo_frame_tag 'new_search' do %>
  <h1 class="text-center"><%= @search.query %></h1>
  <% @search.torrent_links.each do |link| %>
    <ul class="list-group-numbered">
      <li><%= link_to link.name, link.link_forum, class: "link-dark" %>
        |<%= link_to link.hd, link.link, class: "link-primary" %>|
        |<%= color_link_seed(link.seeds) %>|
        |<%= color_link_lychee(link.lychee) %>|
      </li>
    </ul>
  <% end %>
  <div class="text-end">
    <nav aria-label="Page navigation example">
      <ul class="pagination">
        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
        <li class="page-item"><a class="page-link" href="#">1</a></li>
        <li class="page-item"><a class="page-link" href="#">2</a></li>
        <li class="page-item"><a class="page-link" href="#">3</a></li>
        <li class="page-item"><a class="page-link" href="#">Next</a></li>
      </ul>
    </nav>
  </div>
<% end %>
